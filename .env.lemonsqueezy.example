# Add these to your .env.local file for LemonSqueezy integration

# LemonSqueezy API Configuration
LEMONSQUEEZY_API_KEY=your_lemonsqueezy_api_key_here
LEMONSQUEEZY_STORE_ID=your_store_id_here
LEMONSQUEEZY_WEBHOOK_SECRET=your_webhook_secret_here

# Product Variant IDs from LemonSqueezy Dashboard
LEMONSQUEEZY_PRO_MONTHLY_VARIANT_ID=your_pro_monthly_variant_id
LEMONSQUEEZY_PRO_YEARLY_VARIANT_ID=your_pro_yearly_variant_id
LEMONSQUEEZY_ENTERPRISE_MONTHLY_VARIANT_ID=your_enterprise_monthly_variant_id
LEMONSQUEEZY_ENTERPRISE_YEARLY_VARIANT_ID=your_enterprise_yearly_variant_id

# App URL for redirect after payment
NEXT_PUBLIC_APP_URL=http://localhost:3000